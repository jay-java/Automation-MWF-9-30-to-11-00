package core;

public class P005_Conditions {
	public static void main(String[] args) {
		// 1.if else
		int a = 20, b = 12;
		if(a > b) {
			System.out.println("a is greater than b");
		}
		else {
			System.out.println("b is greater than a");
		}
		// 2.nested if
		int age = 34;
		if(age > 18) {
			
		}
		// 3.else if ladder
		// 4.switch case
	}
}
