package core;

public class P006_Loop {
	public static void main(String[] args) {
		//1.for loop ->for(initialization;condition;incre/decre){body}
		//( < , ++) / ( > , --)
		for(int i=1;i<=10;i++) {
			System.out.println(i);
		}

		for(int i=10;i>=1;i++) {
			System.out.println(i);
		}
		//2.while loop -> while(condition){body}
		
		
		//3.do while loop -> do {body} while();
		//4.for each
	}
}
